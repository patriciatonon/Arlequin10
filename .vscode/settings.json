        "buildType": {
            "default": "debug",
            "description": "The build type.",
            "choices": {
                "debug": {
                    "short": "Debug",
                    "long": "Disable optimizations - include debug information.",
                    "buildType": "Debug",
                    "settings": {
                        "PETSC_ARCH": "arch-linux2-c-debug"
                    },
                    "env": {
                        "PETSC_ARCH": "arch-linux2-c-debug"
                    }
                },
                "release": {
                    "short": "Release",
                    "long": "Optimize for speed - exclude debug information.",
                    "buildType": "Release",
                    "settings": {
                        "PETSC_ARCH": "arch-linux2-c-opt"
                    },
                    "env": {
                        "PETSC_ARCH": "arch-linux2-c-opt"
                    }
                }
            }
        }
